{
  "hosting": {
    "public": "functions/static",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "storybook/**",
        "function": "storybook"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }
}
